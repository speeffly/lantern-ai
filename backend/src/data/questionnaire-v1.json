{
  "version": "v1",
  "title": "Lantern AI Career Questionnaire",
  "description": "Comprehensive career exploration questionnaire for high school students",
  "sections": [
    {
      "id": "basic_info",
      "title": "Basic Information",
      "questions": [
        {
          "id": "grade",
          "text": "What grade are you currently in?",
          "type": "single_choice",
          "required": true,
          "options": ["9th", "10th", "11th", "12th"]
        },
        {
          "id": "zipCode",
          "text": "What is your ZIP code?",
          "type": "text",
          "required": true,
          "validation": {
            "pattern": "^\\d{5}$",
            "message": "Please enter a valid 5-digit ZIP code"
          },
          "placeholder": "12345"
        }
      ]
    },
    {
      "id": "work_environment",
      "title": "Work Environment Preferences",
      "questions": [
        {
          "id": "workEnvironment",
          "text": "Where do you see yourself working most comfortably? (Select all that apply)",
          "type": "multi_select",
          "required": true,
          "options": [
            "Outdoors (construction sites, farms, parks)",
            "Indoors (offices, hospitals, schools)",
            "A mix of indoor and outdoor work",
            "From home / remote",
            "Traveling to different locations"
          ]
        }
      ]
    },
    {
      "id": "work_style",
      "title": "Work Style",
      "questions": [
        {
          "id": "workStyle",
          "text": "How do you prefer to work? (Select all that apply)",
          "type": "multi_select",
          "required": true,
          "options": [
            "Building, fixing, or working with tools",
            "Helping people directly",
            "Working with computers or technology",
            "Working with numbers, data, or analysis",
            "Creating designs, art, music, or media"
          ]
        }
      ]
    },
    {
      "id": "thinking_style",
      "title": "Thinking Style",
      "questions": [
        {
          "id": "thinkingStyle",
          "text": "What kinds of problems do you enjoy solving? (Select all that apply)",
          "type": "multi_select",
          "required": true,
          "options": [
            "Troubleshooting and fixing things",
            "Helping people overcome challenges",
            "Understanding how systems or machines work",
            "Inventing or designing new solutions",
            "Planning, organizing, or managing projects"
          ]
        }
      ]
    },
    {
      "id": "education_training",
      "title": "Education & Training",
      "questions": [
        {
          "id": "educationWillingness",
          "text": "How much education or training are you willing to pursue after high school?",
          "type": "single_choice",
          "required": true,
          "options": [
            "Start working right after high school",
            "A few months to 2 years (certifications or training)",
            "2â€“4 years (college or technical school)",
            "4+ years (college and possibly graduate school)",
            "I'm not sure yet"
          ]
        }
      ]
    },
    {
      "id": "academic_interests",
      "title": "Academic Interests",
      "questions": [
        {
          "id": "academicInterests",
          "text": "Which subjects do you enjoy learning? (Select all that apply)",
          "type": "multi_select",
          "required": true,
          "options": [
            "Math",
            "Science (Biology, Chemistry, Physics)",
            "English / Language Arts",
            "Social Studies / History",
            "Art / Creative Subjects",
            "Physical Education / Health",
            "Technology / Computer Science",
            "Foreign Languages",
            "Business / Economics"
          ]
        }
      ]
    },
    {
      "id": "academic_performance",
      "title": "Academic Performance",
      "questions": [
        {
          "id": "academicPerformance",
          "text": "Rate your performance in each subject:",
          "type": "matrix",
          "required": true,
          "subjects": [
            "Math",
            "Science (Biology, Chemistry, Physics)",
            "English / Language Arts",
            "Social Studies / History",
            "Art / Creative Subjects",
            "Physical Education / Health",
            "Technology / Computer Science",
            "Foreign Languages",
            "Business / Economics"
          ],
          "options": [
            "Excellent",
            "Good",
            "Average",
            "Needs Improvement",
            "Haven't taken yet"
          ]
        }
      ]
    },
    {
      "id": "interests_experience",
      "title": "Interests & Experience",
      "questions": [
        {
          "id": "interests",
          "text": "What are your main interests and hobbies?",
          "type": "text_area",
          "required": false,
          "placeholder": "Tell us about what you enjoy doing in your free time, what topics fascinate you, or what activities you're passionate about..."
        },
        {
          "id": "experience",
          "text": "Describe any work, volunteer, or club experience you have:",
          "type": "text_area",
          "required": false,
          "placeholder": "Include part-time jobs, internships, volunteer work, clubs, or other activities. If none yet, write 'None yet' and tell us what you'd like to try..."
        }
      ]
    },
    {
      "id": "personality_traits",
      "title": "Personality & Traits",
      "questions": [
        {
          "id": "traits",
          "text": "Which traits best describe you? (Select all that apply)",
          "type": "multi_select",
          "required": true,
          "options": [
            "Creative and artistic",
            "Analytical and logical",
            "Compassionate and caring",
            "Leadership-oriented",
            "Detail-oriented and organized",
            "Adventurous and willing to take risks",
            "Patient and persistent",
            "Outgoing and social",
            "Independent and self-reliant",
            "Collaborative and team-focused",
            "Curious and inquisitive",
            "Practical and hands-on"
          ]
        },
        {
          "id": "otherTraits",
          "text": "Other traits that describe you (optional):",
          "type": "text_area",
          "required": false,
          "placeholder": "Describe any other personality traits or characteristics that define you..."
        }
      ]
    },
    {
      "id": "values",
      "title": "Values",
      "questions": [
        {
          "id": "incomeImportance",
          "text": "How important is earning a high income to you?",
          "type": "single_choice",
          "required": true,
          "options": ["Very important", "Somewhat important", "Not very important", "Not sure"]
        },
        {
          "id": "stabilityImportance",
          "text": "How important is job stability and security to you?",
          "type": "single_choice",
          "required": true,
          "options": ["Very important", "Somewhat important", "Not very important", "Not sure"]
        },
        {
          "id": "helpingImportance",
          "text": "How important is helping others in your career?",
          "type": "single_choice",
          "required": true,
          "options": ["Very important", "Somewhat important", "Not very important", "Not sure"]
        }
      ]
    },
    {
      "id": "lifestyle_constraints",
      "title": "Lifestyle & Constraints",
      "questions": [
        {
          "id": "constraints",
          "text": "Which of these factors are important to you? (Select all that apply)",
          "type": "multi_select",
          "required": false,
          "options": [
            "Start earning money as soon as possible",
            "Flexible hours",
            "Predictable hours",
            "Stay close to home",
            "Open to relocating",
            "Physical work may be difficult for me",
            "No major constraints"
          ]
        }
      ]
    },
    {
      "id": "decision_readiness",
      "title": "Decision Readiness & Risk",
      "questions": [
        {
          "id": "decisionPressure",
          "text": "How much pressure do you feel to decide on a career path?",
          "type": "single_choice",
          "required": true,
          "options": [
            "Just exploring options",
            "Want to narrow this year",
            "Need a plan soon",
            "Ready to confirm path"
          ]
        },
        {
          "id": "riskTolerance",
          "text": "How comfortable are you with taking risks in your career?",
          "type": "single_choice",
          "required": true,
          "options": [
            "Very comfortable with risk",
            "Somewhat comfortable",
            "Prefer stability",
            "Not sure"
          ]
        }
      ]
    },
    {
      "id": "support_confidence",
      "title": "Support & Confidence",
      "questions": [
        {
          "id": "supportLevel",
          "text": "What level of family/financial support do you have for education or training?",
          "type": "single_choice",
          "required": true,
          "options": [
            "Strong family/financial support",
            "Some support available",
            "Limited support",
            "Not sure about support"
          ]
        },
        {
          "id": "careerConfidence",
          "text": "How confident do you feel about making career decisions?",
          "type": "single_choice",
          "required": true,
          "options": [
            "Very confident",
            "Somewhat confident",
            "Unsure",
            "Very unsure"
          ]
        }
      ]
    },
    {
      "id": "reflection",
      "title": "Reflection",
      "questions": [
        {
          "id": "impactStatement",
          "text": "How do you want to make an impact in the world? (Optional)",
          "type": "text_area",
          "required": false,
          "placeholder": "Think about the legacy you want to leave or how you want to be remembered..."
        },
        {
          "id": "inspiration",
          "text": "Who or what inspires you and why? (Optional)",
          "type": "text_area",
          "required": false,
          "placeholder": "Tell us about someone who inspires you or what motivates you to succeed..."
        }
      ]
    }
  ]
}