{
  "version": "v1",
  "title": "Lantern AI Career Assessment",
  "description": "Career exploration questionnaire for high school students",
  "questions": [
    {
      "id": "q1_grade",
      "type": "single_select",
      "label": "What grade are you currently in?",
      "options": ["9", "10", "11", "12"],
      "required": true
    },
    {
      "id": "q2_zip",
      "type": "text",
      "label": "What is your ZIP code?",
      "required": true
    },
    {
      "id": "q3_work_environment",
      "type": "multi_select",
      "label": "Where do you see yourself working most comfortably?",
      "options": [
        { "key": "outdoors", "label": "Outdoors (construction sites, farms, parks)" },
        { "key": "indoors", "label": "Indoors (offices, hospitals, schools)" },
        { "key": "mixed", "label": "A mix of indoor and outdoor work" },
        { "key": "remote", "label": "From home / remote" },
        { "key": "traveling", "label": "Traveling to different locations" }
      ],
      "required": true
    },
    {
      "id": "q4_work_style",
      "type": "multi_select",
      "label": "How do you prefer to work?",
      "options": [
        { "key": "tools", "label": "Building, fixing, or working with tools" },
        { "key": "helping", "label": "Helping people directly" },
        { "key": "technology", "label": "Working with computers or technology" },
        { "key": "data", "label": "Working with numbers, data, or analysis" },
        { "key": "creative", "label": "Creating designs, art, music, or media" }
      ],
      "required": true
    },
    {
      "id": "q5_thinking_style",
      "type": "multi_select",
      "label": "What kinds of problems do you enjoy solving?",
      "options": [
        { "key": "fixing", "label": "Troubleshooting and fixing things" },
        { "key": "helping_challenges", "label": "Helping people overcome challenges" },
        { "key": "systems", "label": "Understanding how systems or machines work" },
        { "key": "creating", "label": "Inventing or designing new solutions" },
        { "key": "organizing", "label": "Planning, organizing, or managing projects" }
      ],
      "required": true
    },
    {
      "id": "q6_education_willingness",
      "type": "single_select",
      "label": "How much education or training are you willing to pursue after high school?",
      "options": [
        "Work immediately after high school",
        "A few months to 2 years (certifications or training)",
        "2â€“4 years (college or technical school)",
        "4+ years (college and possibly graduate school)",
        "I'm not sure yet"
      ],
      "required": true
    },
    {
      "id": "q7_academic_interests",
      "type": "multi_select",
      "label": "Which subjects do you enjoy learning?",
      "options": [
        "Math",
        "Science",
        "English / Language Arts",
        "Social Studies / History",
        "Art / Creative Subjects",
        "Physical Education / Health",
        "Technology / Computer Science",
        "Foreign Languages",
        "Business / Economics"
      ],
      "required": true
    },
    {
      "id": "q8_academic_performance",
      "type": "matrix",
      "label": "How would you rate your performance in the following subjects?",
      "rows": [
        "Math",
        "Science",
        "English / Language Arts",
        "Social Studies / History",
        "Art / Creative Subjects",
        "Physical Education / Health",
        "Technology / Computer Science",
        "Foreign Languages",
        "Business / Economics"
      ],
      "columns": [
        "Excellent",
        "Good",
        "Average",
        "Needs Improvement",
        "Haven't taken yet"
      ],
      "required": true
    },
    {
      "id": "q9_interests_text",
      "type": "text_long",
      "label": "What are your interests or hobbies?",
      "required": false
    },
    {
      "id": "q10_experience_text",
      "type": "text_long",
      "label": "What work, volunteer, or hands-on experience do you have?",
      "required": false
    },
    {
      "id": "q11_traits",
      "type": "multi_select",
      "label": "Which traits best describe you?",
      "options": [
        { "key": "creative", "label": "Creative and artistic" },
        { "key": "analytical", "label": "Analytical and logical" },
        { "key": "compassionate", "label": "Compassionate and caring" },
        { "key": "leadership", "label": "Leadership-oriented" },
        { "key": "detail_oriented", "label": "Detail-oriented and organized" },
        { "key": "adventurous", "label": "Adventurous and willing to take risks" },
        { "key": "patient", "label": "Patient and persistent" },
        { "key": "outgoing", "label": "Outgoing and social" },
        { "key": "independent", "label": "Independent and self-reliant" },
        { "key": "collaborative", "label": "Collaborative and team-focused" },
        { "key": "curious", "label": "Curious and inquisitive" },
        { "key": "hands_on", "label": "Practical and hands-on" }
      ],
      "required": true
    },
    {
      "id": "q12_income_importance",
      "type": "single_select",
      "label": "How important is earning a high income to you?",
      "options": [
        "Very important",
        "Somewhat important",
        "Not very important",
        "I'm not sure yet"
      ],
      "required": true
    },
    {
      "id": "q13_stability_importance",
      "type": "single_select",
      "label": "How important is job stability and security to you?",
      "options": [
        "Very important",
        "Somewhat important",
        "Not very important"
      ],
      "required": true
    },
    {
      "id": "q14_helping_importance",
      "type": "single_select",
      "label": "How important is helping others in your future career?",
      "options": [
        "Very important",
        "Somewhat important",
        "Not very important",
        "I'm not sure yet"
      ],
      "required": true
    },
    {
      "id": "q15_constraints",
      "type": "multi_select",
      "label": "Are there any factors we should consider when suggesting careers?",
      "options": [
        "Start earning money as soon as possible",
        "Flexible hours",
        "Predictable hours",
        "Stay close to home",
        "Open to relocating",
        "Physical work may be difficult for me",
        "No major constraints"
      ],
      "required": false
    },
    {
      "id": "q16_decision_urgency",
      "type": "single_select",
      "label": "How soon do you feel pressure to decide on a career path?",
      "options": [
        "I'm just exploring right now",
        "I want to narrow things down this year",
        "I need a clear plan soon",
        "I already have a path in mind but want to confirm it"
      ],
      "required": true
    },
    {
      "id": "q17_risk_tolerance",
      "type": "single_select",
      "label": "How comfortable are you with career uncertainty or risk?",
      "options": [
        "Very comfortable",
        "Somewhat comfortable",
        "Prefer stability",
        "I'm not sure yet"
      ],
      "required": true
    },
    {
      "id": "q18_support_confidence",
      "type": "single_select",
      "label": "Do you feel you have support for education or training after high school?",
      "options": [
        "Strong support",
        "Some support",
        "Limited support",
        "I'm not sure"
      ],
      "required": true
    },
    {
      "id": "q19_career_confidence",
      "type": "single_select",
      "label": "How confident do you feel about your future career direction?",
      "options": [
        "Very confident",
        "Somewhat confident",
        "Unsure",
        "Very unsure"
      ],
      "required": true
    },
    {
      "id": "q20_impact_text",
      "type": "text_long",
      "label": "How do you want to be remembered or make an impact?",
      "required": false
    },
    {
      "id": "q21_inspiration_text",
      "type": "text_long",
      "label": "Who inspires you and why?",
      "required": false
    }
  ]
}